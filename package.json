{
	"name": "terminal-loader",
	"displayName": "Terminal Loader",
	"description": "Open multiple terminals in a specific location and run command",
	"version": "1.0.2",
	"publisher": "ariassd",
	"icon": "assets/ico.png",
	"homepage": "https://github.com/ariassd/vscode-load-terminals/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ariassd/vscode-load-terminals"
    },
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onCommand:extension.loadTerminals"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.loadTerminals",
				"title": "Load Terminals"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
